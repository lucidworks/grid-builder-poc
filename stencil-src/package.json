{
  "name": "grid-builder-stencil",
  "version": "1.0.0",
  "description": "StencilJS variant of grid builder POC",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test.watch": "jest --config jest.config.js --watch",
    "test-unit": "jest --config jest.config.js",
    "generate": "stencil generate",
    "serve": "http-server ../stencil -p 8080 -c-1 --cors -o",
    "prettier-css-all": "prettier --parser css --write 'src/**/*.css'",
    "stylelint-all": "stylelint --fix 'src/**/*.css'",
    "prettier-ts-all": "prettier --write 'src/**/*.{ts,tsx}'",
    "tslint-all": "tslint --fix --project tsconfig.json 'src/**/*.{ts,tsx}'",
    "lint-ts:all": "npm run prettier-ts-all && npm run tslint-all",
    "lint-css:all": "npm run prettier-css-all && npm run stylelint-all"
  },
  "dependencies": {
    "@interactjs/actions": "^1.10.27",
    "@interactjs/interactjs": "^1.10.27",
    "@stencil/store": "^2.0.0",
    "interactjs": "^1.10.19"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@stencil/core": "^4.8.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^20.0.0",
    "babel-jest": "^29.5.0",
    "http-server": "^14.1.1",
    "jest": "27.4.5",
    "jest-cli": "27.4.5",
    "jest-environment-jsdom": "^27.5.1",
    "jest-jasmine2": "^27.5.1",
    "jsdom": "^16.7.0",
    "prettier": "^2.8.8",
    "puppeteer": "^21.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.7.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "tslint-sonarts": "^1.9.0"
  },
  "license": "MIT"
}
