{
  "name": "grid-builder-stencil",
  "version": "1.0.0",
  "description": "StencilJS variant of grid builder POC",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test.watch": "jest --config jest.config.js --watch",
    "test-unit": "jest --config jest.config.js",
    "generate": "stencil generate",
    "serve": "http-server ../stencil -p 8080 -c-1 --cors -o",
    "prettier-css-all": "prettier --parser css --write 'src/**/*.css'",
    "stylelint-all": "stylelint --fix 'src/**/*.css'",
    "prettier-ts-all": "prettier --write 'src/**/*.{ts,tsx}'",
    "eslint-all": "eslint -c eslint.config.js --fix 'src/**/*.{ts,tsx}'",
    "lint-ts:all": "npm run prettier-ts-all && npm run eslint-all",
    "lint-css:all": "npm run prettier-css-all && npm run stylelint-all"
  },
  "dependencies": {
    "@interactjs/actions": "^1.10.27",
    "@interactjs/interactjs": "^1.10.27",
    "@stencil/store": "^2.0.0",
    "interactjs": "^1.10.19"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@stencil/core": "^4.8.2",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@stylistic/eslint-plugin-js": "^2.12.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^20.0.0",
    "@typescript-eslint/parser": "^8.18.0",
    "babel-jest": "^29.5.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.14.0",
    "http-server": "^14.1.1",
    "jest": "27.4.5",
    "jest-cli": "27.4.5",
    "jest-environment-jsdom": "^27.5.1",
    "jest-jasmine2": "^27.5.1",
    "jsdom": "^16.7.0",
    "prettier": "^3.4.2",
    "puppeteer": "^24.30.0",
    "stylelint": "^14.16.1",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.7.0",
    "typescript-eslint": "^8.18.1"
  },
  "license": "MIT"
}
